{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true 
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
        
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "prettier",
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "no-console": "warn"
    }
}
